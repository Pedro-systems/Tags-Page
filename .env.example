# Variáveis de ambiente para conexão com Supabase
# Copie este arquivo para .env.local e preencha com suas credenciais

# URL do projeto Supabase (encontre em Settings > API)
NEXT_PUBLIC_SUPABASE_URL=sua_url_do_supabase_aqui

# Chave anônima/pública do Supabase (encontre em Settings > API)
NEXT_PUBLIC_SUPABASE_ANON_KEY=sua_chave_anonima_aqui
